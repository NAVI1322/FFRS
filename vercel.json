{
    "routes": [
      {
        "src": "^/assets/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/assets/$1"
      },
      {
        "src": "^/images/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/images/$1"
      },
      {
        "src": "^/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "^/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "^/vite.svg",
        "dest": "/vite.svg"
      },
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/(services|about|contact|testimonials)/?$",
        "dest": "/index.html"
      },
      {
        "src": "^/services#([0-9]+)$",
        "dest": "/index.html"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite"
  }