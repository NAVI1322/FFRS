{
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/services",
        "dest": "/index.html"
      },
      {
        "src": "/about",
        "dest": "/index.html"
      },
      {
        "src": "/contact",
        "dest": "/index.html"
      },
      {
        "src": "/testimonials",
        "dest": "/index.html"
      },
      {
        "src": "^/assets/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/assets/$1"
      },
      {
        "src": "^/images/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/images/$1"
      },
      {
        "src": "^/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite"
  }